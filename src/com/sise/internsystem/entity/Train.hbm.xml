<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sise.internsystem.entity">

	<class name="Train" table="t_train">
		<id name="id">
            <generator class="native"/>
		</id>
		<property name="name" />
		<property name="no" />
		<property name="driver" />
		<property name="infos" />
	</class>
	
</hibernate-mapping>
