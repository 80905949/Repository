<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sise.internsystem.entity">

	<class name="Trainpath" table="t_trainpath">
		<id name="id">
            <generator class="native"/>
		</id>
		<property name="date" />
		<property name="times" />
		<property name="train" />
		<property name="endtime" />
		<property name="fromplace" />
		<property name="toplace" />
	</class>
	
</hibernate-mapping>
